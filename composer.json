{
    "name": "phpixie/social",
    "description": "PHPixie library for social OAuth APis: facebook, twitter, google and vk",
    "keywords": ["social", "auth", "facebook", "twitter", "google", "vk", "oauth"],
    "homepage": "http://phpixie.com",
    "type": "library",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Roman Tsiupa",
            "email": "draconyster@gmail.com",
            "homepage": "http://dracony.org"
        }
    ],
    "require": {
        "phpixie/slice": "~3.0"
    },
    "require-dev": {
        "phpixie/test": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "PHPixie\\": "src/PHPixie",
            "PHPixie\\Tests\\": "tests/PHPixie/Tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    }
}
